<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zzheng.helloworld" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DisplayMessageActivity"
            android:label="@string/title_activity_display_message"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.zzheng.helloworld.MainActivity" />
        </activity>
        <activity
            android:name=".NewLogActivity"
            android:label="@string/title_activity_new_log"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.zzheng.helloworld.MainActivity" />
        </activity>
        <activity
            android:name=".ViewHistoryActivity"
            android:label="@string/title_activity_view_history"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.zzheng.helloworld.MainActivity" />
        </activity>
        <activity
            android:name=".EditBubbleActivity"
            android:label="@string/title_activity_edit_bubble"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.zzheng.helloworld.MainActivity" />
        </activity>
    </application>

</manifest>
