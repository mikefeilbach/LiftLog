<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:actionBarStyle">@style/MyActionBar</item>

        <!-- Support library compatibility -->
        <item name="actionBarStyle">@style/MyActionBar</item>
    </style>






    <!-- Base application theme. -->
    <style name="AppTheme2" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:actionBarStyle">@style/MyActionBar</item>

        <!-- Support library compatibility -->
        <item name="actionBarStyle">@style/MyActionBar2</item>
    </style>

    <!-- Base application theme. -->
    <style name="AppTheme3" parent="Theme.AppCompat.Light.DarkActionBar">
    </style>
    <!-- ActionBar styles -->
    <style name="MyActionBar"
        parent="@style/Widget.AppCompat.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@drawable/ab2</item>

        <!-- Support library compatibility -->
        <item name="background">@drawable/ab2</item>
    </style>

    <!-- ActionBar styles -->
    <style name="MyActionBar2"
        parent="@style/Widget.AppCompat.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@drawable/ab3</item>

        <!-- Support library compatibility -->
        <item name="background">@drawable/ab3</item>
    </style>
</resources>
